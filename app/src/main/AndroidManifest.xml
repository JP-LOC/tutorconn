<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tutorscape">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ByLocation"></activity>
        <activity
            android:name=".About"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyTutorsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BaseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TutorProfile"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TutorListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginTutor"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginStudent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterTutor"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterStudent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WelcomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>